          "commands/**.md",
          "concepts/**.md",
          "**/toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "Default",
      "StaticToc",
      "../templates/leap"
    ]
  }
}